{
    "name": "oh-my-dog",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "dotenv -e .env.dev -- vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "postinstall": "prisma generate",
        "dev:prisma-gen": "dotenv -e .env.dev -- npx prisma generate",
        "dev:prisma-push": "dotenv -e .env.dev -- npx prisma db push",
        "prod:db-push": "npx prisma db push"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^2.0.0",
        "@sveltejs/kit": "^1.5.0",
        "@tailwindcss/forms": "^0.5.3",
        "@types/nodemailer": "^6.4.7",
        "autoprefixer": "^10.4.14",
        "postcss": "^8.4.23",
        "prisma": "^4.13.0",
        "svelte": "^3.54.0",
        "svelte-check": "^3.0.1",
        "sveltekit-superforms": "^0.8.6",
        "tailwindcss": "^3.3.2",
        "tslib": "^2.4.1",
        "typescript": "^5.0.0",
        "vite": "^4.3.0",
        "zod": "^3.21.4"
    },
    "type": "module",
    "dependencies": {
        "@lucia-auth/adapter-prisma": "^2.0.0",
        "@prisma/client": "^4.14.0",
        "cloudinary": "^1.36.4",
        "lucia-auth": "^1.5.0",
        "nodemailer": "^6.9.1"
    }
